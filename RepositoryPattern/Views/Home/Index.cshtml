@{
    var emps = ViewBag.data;
}
@* 
<h1>EmpId: @emps.EmpId</h1>
<h1>EmpId: @emps.EmpName</h1>
<h1>EmpId: @emps.Salary</h1> *@

@{
    foreach (var item in emps)
    {
        <h1>EmpId: @item.EmpId</h1>
        <h1>EmpId: @item.EmpName</h1>
        <h1>EmpId: @item.Salary</h1>
    }
}

